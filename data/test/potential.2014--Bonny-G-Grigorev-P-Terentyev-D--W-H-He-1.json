        "description": {
            "citation": {
                "document-type": "journal", 
                "title": "On the binding of nanometric hydrogen-helium clusters in tungsten", 
                "author": [
                    {
                        "givenname": "G.", 
                        "surname": "Bonny"
                    }, 
                    {
                        "givenname": "P.", 
