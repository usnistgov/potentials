        "description": {
            "citation": {
                "document-type": "journal", 
                "title": "On the mobility of vacancy clusters in reduced activation steels: an atomistic study in the Fe-Cr-W model alloy", 
                "author": [
                    {
                        "givenname": "G.", 
                        "surname": "Bonny"
                    }, 
                    {
                        "givenname": "N.", 
                        "surname": "Castin"
                    }, 
